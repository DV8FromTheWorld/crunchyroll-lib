    "base64-js": "^1.2.3",
    "big-integer": "^1.6.26",
    "pako": "^1.0.6",
    "sax": "^1.2.4",
    "text-encoding": "^0.6.4"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
